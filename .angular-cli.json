{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "pheyi-ng"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "Content/home-mobile/index-mobile.css",
        "css/vendor/material-icons.min.css",
        "css/ionicons-2.0.1/css/ionicons.css",
        "css/vendor/socicon.min.css",
        "css/vendor/bootstrap.min.css",
        "css/theme.min.css",
        "Content/animate.min.css",
        "Content/Site.css"
     

      ],
      "scripts": [      
        "js/vendor/jquery-2.1.4.min.js",
        "js/vendor/bootstrap.min.js",
        "js/vendor/smoothscroll.js",
        "js/vendor/velocity.min.js",
        "js/vendor/waves.min.js",
        "js/vendor/icheck.min.js",
        "js/vendor/owl.carousel.min.js",
        "js/vendor/jquery.downCount.js",
        "js/vendor/magnific-popup.min.js",
        "js/scripts.js",
        "js/jivo.js",
        "js/google-analytics.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
